## VictoriaMetrics
VM_RETENTION=7d                      # срок хранения метрик

## JSON exporter (Xray /debug/vars)
EXPORTER_SOURCE=http://<host>:11111/debug/vars  # URL /debug/vars Xray
EXPORTER_TIMEOUT=5                  # таймаут запроса, сек
EXPORTER_DELAY_UNIT=ms              # единицы задержки observatory: ms|s
EXPORTER_LOG_LEVEL=INFO             # уровень логов экспортера

## Xray API (опционально, если есть HTTP JSON API)
XRAY_API_SOURCE=                    # URL API с uptime/online (если есть)
XRAY_API_TIMEOUT=5                  # таймаут запроса, сек

## 3x-ui API (login + inbounds)
UI_HOST=api.example.com             # домен панели 3x-ui
UI_PORT=443                         # порт панели
UI_BASEPATH=/                       # basepath панели (например / или /panel)
UI_SCHEME=https                     # http|https
UI_USERNAME=                        # логин 3x-ui
UI_PASSWORD=                        # пароль 3x-ui
UI_BEARER_TOKEN=                    # bearer token (если включён)
UI_API_KEY=                         # apiKey header (если требуется)
UI_LOGIN_PATH=/login                # путь логина
UI_INBOUNDS_PATH=/panel/api/inbounds/list  # путь списка inbounds
UI_ONLINE_PATH=/panel/api/inbounds/onlines # путь online (если есть)
UI_INSECURE=false                   # true для отключения TLS проверки

## 3x-ui WebSocket (uptime/online из WS)
UI_WS_URL=wss://api.example.com/ws  # WS endpoint
UI_WS_TIMEOUT=5                     # таймаут чтения, сек
UI_WS_CACHE_TTL=30                  # кэш WS, сек
UI_WS_MESSAGES=3                    # сколько сообщений читать

## Grafana
GRAFANA_HTTP_ADDR=0.0.0.0           # адрес бинда Grafana
GRAFANA_DOMAIN=grafana.yourdomain.com  # домен для Traefik

## xray-checker
XRAY_CHECKER_SUBSCRIPTION_URL=https://your-subscription-url/sub  # URL подписки
